{
  "name": "alpacasalon-backend",
  "version": "0.1.0",
  "description": "alpacasalon-backend",
  "homepage": "https://github.com/rmfpdlxmtidl/alpacasalon-backend#readme",
  "bugs": {
    "url": "https://github.com/rmfpdlxmtidl/alpacasalon-backend/issues",
    "email": "alpacasalon@alpacasalon.com"
  },
  "license": "GPL-3.0-only",
  "author": "Alpaca Salon <alpacasalon@alpacasalon.com>",
  "main": "dist/index.js",
  "repository": "https://github.com/rmfpdlxmtidl/alpacasalon-backend.git",
  "scripts": {
    "clean": "rm -rf .yarn/cache .yarn/unplugged .yarn/install-state.gz coverage dist .pnp.cjs yarn.lock",
    "dev": "NODE_ENV=development concurrently \"webpack\" \"nodemon -r dotenv/config dist/index.js dotenv_config_path=.env.development.local --trace-warnings\"",
    "build": "NODE_ENV=production webpack",
    "start": "NODE_ENV=production node -r dotenv/config dist/index.js dotenv_config_path=.env.local",
    "test": "NODE_ENV=test jest",
    "format": "prettier . --write --ignore-path .gitignore",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "type": "tsc --pretty --noEmit",
    "pre-push": "yarn type",
    "generate": "rm -rf src/graphql/generated && graphql-codegen --config codegen.yml",
    "generate-db": "database/generate",
    "export": "database/export",
    "import": "database/import",
    "postinstall": "[ -d '.husky' ] && husky install || true"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.16.1",
    "@graphql-tools/merge": "^8.2.1",
    "apollo-server-core": "^3.6.1",
    "apollo-server-errors": "^3.3.0",
    "apollo-server-express": "^3.6.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "graphql": "^15.8.0",
    "graphql-scalars": "^1.14.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "multer": "^1.4.4",
    "nanoid": "^3.2.0",
    "node-fetch": "^3.1.0",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.7.1",
    "redis": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-resolvers": "2.4.3",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.19",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/pg": "^8.6.3",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vramework/schemats": "^0.0.12",
    "axios": "^0.24.0",
    "babel-jest": "^27.4.6",
    "concurrently": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.6",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "terser-webpack-plugin": "^5.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "private": true,
  "packageManager": "yarn@3.1.1"
}
